 class Demo{
    public String Add(int a,int b){
        if(a%2==0){
        return "even";
        }
        return "odd";
    }
    }

class Main {
    public static void main(String[] args) {
        Demo obj=new Demo();
        String a=obj.Add(10,5);
        
        System.out.println(a);
    }
}
